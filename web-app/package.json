{
  "name": "simrigmanager-web-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --hmr --host 0.0.0.0",
    "dev": "docker compose up -d",
    "build": "ng build --configuration=production",
    "copywebconfig": "copyfiles -f ./web.config ./dist/simrigmanager-web-app",
    "postbuild": "yarn run copywebconfig",
    "prod": "ng build --configuration=production && serve -s dist/simrigmanager-web-app -l 4201",
    "test": "ng test",
    "lint": "ng lint",
    "prettify": "prettier --write \"./src/**/*.{js,jsx,ts,tsx,css,scss,json}\"",
    "prune": "rm -rf ./.angular && rm -rf ./dist && docker-compose down && docker system prune -a && docker volume prune",
    "reinstall": "rm -rf ./node_modules && rm yarn.lock && yarn install",
    "check-for-updates": "npx npm-check-updates -u"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.0.5",
    "@angular/cdk": "^18.2.8",
    "@angular/common": "^19.0.5",
    "@angular/compiler": "^19.0.5",
    "@angular/core": "^19.0.5",
    "@angular/forms": "^19.0.5",
    "@angular/localize": "^19.0.5",
    "@angular/material": "^18.2.8",
    "@angular/platform-browser": "^19.0.5",
    "@angular/platform-browser-dynamic": "^19.0.5",
    "@angular/router": "^19.0.5",
    "@angular/service-worker": "^19.0.5",
    "@apollo/client": "^3.11.8",
    "@ng-bootstrap/ng-bootstrap": "^17.0.1",
    "@ngrx/effects": "^18.1.0",
    "@ngrx/entity": "^18.1.0",
    "@ngrx/store": "^18.1.0",
    "@popperjs/core": "^2.11.8",
    "apollo-angular": "^7.2.0",
    "bootstrap-dark-5": "^1.1.3",
    "bootstrap-icons": "^1.11.3",
    "graphql": "^16.9.0",
    "graphql-ws": "^5.16.0",
    "jquery": "^3.7.1",
    "lodash-es": "^4.17.21",
    "moment": "^2.30.1",
    "postcss": "^8.4.47",
    "rxjs": "~7.8.1",
    "tslib": "^2.7.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.0.6",
    "@angular-eslint/builder": "18.3.1",
    "@angular-eslint/eslint-plugin": "18.3.1",
    "@angular-eslint/eslint-plugin-template": "18.3.1",
    "@angular-eslint/schematics": "18.3.1",
    "@angular-eslint/template-parser": "18.3.1",
    "@angular/cli": "^19.0.6",
    "@angular/compiler-cli": "^19.0.5",
    "@ngrx/schematics": "^18.1.0",
    "@ngrx/store-devtools": "^18.1.0",
    "@playwright/test": "^1.48.0",
    "@types/jasmine": "~5.1.4",
    "@types/jquery": "^3.5.31",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.7.5",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "copyfiles": "^2.4.1",
    "eslint": "^9.12.0",
    "jasmine-core": "~5.3.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.4.4",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "^2.1.0",
    "prettier": "^3.3.3",
    "serve": "^14.2.3",
    "ts-node": "~10.9.2",
    "typescript": "~5.5.4"
  }
}