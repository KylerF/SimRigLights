{
  "name": "simrigmanager-web-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --hmr --host 0.0.0.0",
    "dev": "docker-compose up -d",
    "build": "ng build --configuration=production",
    "prod": "ng build --configuration=production && serve -s dist/simrigmanager-web-app -l 4201",
    "test": "ng test",
    "e2e": "playwright test",
    "lint": "ng lint",
    "prettify": "prettier --write \"./src/**/*.{js,jsx,ts,tsx,json}\"",
    "prune": "rm -rf ./.angular && rm -rf ./dist && docker-compose down && docker system prune -a && docker volume prune",
    "reinstall": "rm -rf ./node_modules && rm yarn.lock && yarn install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.1.0",
    "@angular/cdk": "^15.1.0",
    "@angular/common": "^15.1.0",
